<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="blocks/quadratic_calculator/db" VERSION="2024010100"
       COMMENT="Install file for the quadratic calculator block">
    <TABLES>
        <TABLE NAME="block_quadratic_calculator" COMMENT="History of quadratic calculations">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true"/>
                <FIELD NAME="a" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="b" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="c" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="x1" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="x2" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="created_at" TYPE="int" LENGTH="10" NOTNULL="true"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
